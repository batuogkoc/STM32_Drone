/**
* @brief This function handles EXTI line[9:5] interrupts.
*/
void EXTI9_5_IRQHandler(void)
{
  /* USER CODE BEGIN EXTI9_5_IRQn 0 */
	this_time = microseconds();
	reading_elapsed_time = this_time - reading_last_time;
	reading_last_time = this_time;
	new_data = 1;
	
	//reading_count++;
  /* USER CODE END EXTI9_5_IRQn 0 */
  HAL_GPIO_EXTI_IRQHandler(GPIO_PIN_8);
  /* USER CODE BEGIN EXTI9_5_IRQn 1 */

  /* USER CODE END EXTI9_5_IRQn 1 */
}

/**
* @brief This function handles TIM1 capture compare interrupt.
*/
void TIM1_CC_IRQHandler(void)
{
  /* USER CODE BEGIN TIM1_CC_IRQn 0 */
	inputCapture = __HAL_TIM_GET_COUNTER(&htim1);
	__HAL_TIM_SET_COUNTER(&htim1,0);
  /* USER CODE END TIM1_CC_IRQn 0 */
  HAL_TIM_IRQHandler(&htim1);
  /* USER CODE BEGIN TIM1_CC_IRQn 1 */
	if(inputCapture > 3000){
		ch_count = 0;
		return;
	}
	else if(ch_count<8){
		ch[ch_count] = inputCapture;
		ch_count++;
	}
	else if(ch_count == 9){
		return;
	}
	last_signal = microseconds();
  /* USER CODE END TIM1_CC_IRQn 1 */
}

